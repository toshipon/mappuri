div(ng-app='ng-app')
  h2 Outings
  div(ng-controller='OutingCtrl')

   form(class='form-inline', ng-submit='addOuting()')
      div
        input(type='text', ng-model='Name', size='30', placeholder='Name')
        input.btn.btn-primary(type='submit', value='add')

    ul
      li(ng-repeat='outing in outings')
        p(ng-model='outing.Id') {{outing.Id}}
        p(ng-model='outing.Name') {{outing.Name}}
